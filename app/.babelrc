{
  "comments": false,
  "presets": [
    "minify",
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 10"]
      }
    }],
    "stage-0"
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "stage-0"
      ],
      "plugins": ["istanbul"]
    }
  },
  "plugins": [
    "transform-object-rest-spread",
    "transform-runtime"
  ]
}
